<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxspringboot.testapp.FinishController">
   <children>
      <Button fx:id="restartBtn" layoutX="71.0" layoutY="305.0" mnemonicParsing="false" onAction="#restart" prefHeight="43.0" prefWidth="100.0" text="Restart">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="exitBtn" layoutX="397.0" layoutY="305.0" mnemonicParsing="false" prefHeight="43.0" onAction="#exit" prefWidth="100.0" text="Exit">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="108.0" layoutY="88.0" prefHeight="27.0" prefWidth="139.0" text="Your score :">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="score"
             layoutX="263.0" layoutY="90.0" prefHeight="17.0" prefWidth="74.0" text="22">
         <font>
            <Font name="Arial Bold Italic" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
